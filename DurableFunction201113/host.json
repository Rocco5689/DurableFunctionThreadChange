{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "console": {
      "isEnabled": "true"
    },
    "logLevel": {
      "default": "Information",
      "DurableTask.AzureStorage": "Information",
      "DurableTask.Core": "Information",
      "Host.Results": "Trace",
      "Function": "Trace",
      "Host.Aggregator": "Trace"
    },
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "TaskHub2021013102",
      "logReplayEvent": true,
      "tracing": {
        "traceInputsAndOutputs": true,
        "traceReplayEvents": true
      }
    }
  }
}